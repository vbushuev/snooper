			}
			sociomanticDetailViewCheck = setInterval(function() {
				if (typeof sociomantic === 'object' &&
					typeof sociomantic.sonar === 'object' &&
					typeof sociomantic.sonar.adv === 'object' &&
					typeof sociomantic.sonar.adv['baby-walz-de'] === 'object')
				{
					sociomanticDetailViewTrack(uid, productObj, currentArticleVO);
					clearInterval(sociomanticDetailViewCheck);
					sociomanticDetailViewCheck = 0;
				}
			},
			250);
		}
	}

