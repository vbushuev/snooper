			}

			// add always the current product at the beginning
			lsp_oldlist.unshift(lsp_data);
		}

		var lsp_newlist = new Array();
		var j 			= 0;
		var output 		= '';

		for (var i = 0; i < lsp_oldlist.length; i++) {

			// copy all preview products to the new list
			// except the current product if it is not at the beginning
			// and write it to the screen - but only not more than 5
			if ((lsp_oldlist[i]['pk'] != lsp_pk || i == 0) && j < 5) {
				lsp_newlist.push(lsp_oldlist[i]);

				output = lsptemplate.replace("###reduced###", lsp_oldlist[i]['reduced']);
				output = output.replace("###link###",         lsp_oldlist[i]['link']+'&emcs0=Zuletzt_Gesehen&emcs1=PDS&emcs2='+lsp_ordernumber+'&emcs3='+lsp_oldlist[i]['onum']);
				output = output.replace("###img###",          lsp_oldlist[i]['img']);
				output = output.replace("###alt###",          lsp_oldlist[i]['alt']);
				output = output.replace("###brand###",        lsp_oldlist[i]['brand']);
				output = output.replace("###name###",         lsp_oldlist[i]['name']);
				output = output.replace("###banner###",       lsp_oldlist[i]['banner']);
				output = output.replace("###oldprice###",     lsp_oldlist[i]['oldprice']);
				output = output.replace("###price###",        lsp_oldlist[i]['price']);

				if (typeof(lsp_oldlist[i]['baseUnitPrice']) != 'undefined') {
					output = output.replace("###withBaseUnitPrice###", 'withBaseUnitPrice');
					output = output.replace("###baseUnitPrice###",     lspBaseUnitTemplate);
					output = output.replace("###llBaseUnit###",        lsp_oldlist[i]['llBaseUnit']);
					output = output.replace("###baseUnit###",          lsp_oldlist[i]['baseUnit']);
					output = output.replace("###baseUnitPrice###",     lsp_oldlist[i]['baseUnitPrice']);
				} else {
					output = output.replace("###withBaseUnitPrice###", '');
					output = output.replace("###baseUnitPrice###", '');
				}
				lsptemplate = lsptemplate.replace("recommendation_first-child", '');

				document.write(output);
				j++;
			}
		}

		lsp_string = JSON.stringify(lsp_newlist);

		if (useStorage) {
			localStorage.setItem('mb3StorageLastSeenProduct', lsp_string);
		} else {
			$.cookie('mb3CookieLastSeenProduct', lsp_string, { path: '/' });
		}

		function supports_html5_storage() {
			try {
				return 'localStorage' in window && window['localStorage'] !== null;
			} catch (e) {
				return false;
			}
		}

		</script>
		</div>
	</div>
	
	
		
	
		<span id="c196616" name="c196616" class="cTypo3"></span>
		
			<div id="campaignposition_12" class="campaignBottomDisp"></div> 
		
			
	
		</div>
        </div>
        
