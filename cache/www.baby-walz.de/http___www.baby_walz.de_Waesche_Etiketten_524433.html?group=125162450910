	var sociomanticDetailViewCheck = 0;

	function sociomanticDetailViewTrack(uid, productObj, currentArticleVO) {
		sociomantic.sonar.adv['baby-walz-de'].clear();
		window.product = {identifier: currentArticleVO.artNumber};
		sociomantic.sonar.adv['baby-walz-de'].track();
