// Econda: AddToBasket
			function addToBasketEconda() {
				if (typeof(emospro.ec_Event) != 'undefined') {
					$('<input>').attr({
						type: 'hidden',
						name: selfCType + '[' + hashId + '][295919252410][groupPath]',
						value: emospro.ec_Event[0][4]
					}).appendTo('#productForm_' + selfUID);
					document.cookie = 'mb3PartnerEconda=' +
						'c_add' + '|' +
						emospro.ec_Event[0][1] + '|' +
						emospro.ec_Event[0][2] + '|' +
						emospro.ec_Event[0][3] + '|' +
						emospro.ec_Event[0][4] + '|' +
						jQuery('#productAmountForm_' + selfUID).val() + '|' +
						emospro.ec_Event[0][6] + '|' +
						emospro.ec_Event[0][7] + '|' +
						emospro.ec_Event[0][8] + ';	path=/';
				}
			}
addToBasketEconda();


// Hurra: AddToBasket
			function addToBasketHurra() {
				if (typeof product01 == 'undefined' && typeof selfUID != 'undefined') {
					var product01 = new dmc.WALZ.Product({'selfUID': selfUID});
					product01 = product01.getInstance();
				}
				currentArticleVO = product01.getCurrentArticle();
				var groupPath = window.partnerContentLabelGrouppath.substr(0, (window.partnerContentLabelGrouppath.length - 1));
				var lastGroupName = groupPath.substr(groupPath.lastIndexOf('/') + 1);

				document.cookie = 'mb3PartnerHurra=' +
					currentArticleVO.artNumber + '|' +
					product01.dataArticles[currentArticleVO.artNumber][currentArticleVO.artSize].currentPricePlain + '|' +
					'add_to_cart' + '|' +
					lastGroupName + '; path=/';
			}
addToBasketHurra();


// Google Tag Manager: AddToBasket
			function addToBasketGTM() {
				dataLayer.push ({
					'event':'add_to_cart'
				});
			}
addToBasketGTM();

		}

