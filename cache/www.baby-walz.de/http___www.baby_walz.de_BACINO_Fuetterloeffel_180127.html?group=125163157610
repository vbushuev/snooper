		<script type="text/javascript">

		var stockTypeText = new Array()
		stockTypeText['']	=  '';
		stockTypeText[0] 	=  'unbekannt';
		stockTypeText[1] 	=  'sofort lieferbar';
		stockTypeText[2] 	=  'wird nachgeliefert';
		stockTypeText[3] 	=  'momentan nicht verfügbar ';
		stockTypeText[4] 	=  'ausverkauft';

		var kqpridata = '';

		// main function processing call and get results from webservice
		function doKqpriProductupdate(data) {
			xajax_getKqpriAvailability(data, '101');
		} // end: function

		// loading-message
		xajax.callback.global.onRequest = function() {
		} // end: function

		// the response of the xajax call
		// updates the availability

		xajax.callback.global.onComplete = function() {
			var uid = '58688';
			var id	= '295257375210';

			// start updating availability if the response (kqpriList) is set
			if (kqpridata.length > 0) {
				// parse the response and build an array with
				// the artnum and size as key and stock,amount and weekstodelivery as values
				// adcode is not required anymore
				var kqpriConfList = new Array();

				var kqpriList = kqpridata.split('||');

				for (var i = 0; i < kqpriList.length; i++) {
					var kqpriValues = kqpriList[i].split('#');

					// If size is NULL, it needs at least to be "0"
					if (!kqpriValues[2]) {
						kqpriValues[2] = 0;
					}

					// artnumber
					if (typeof kqpriConfList[kqpriValues[0]] == 'undefined') {
						kqpriConfList[kqpriValues[0]] = new Array();
					}
					// size
					if (typeof kqpriConfList[kqpriValues[0]][kqpriValues[2]] == 'undefined') {
						 kqpriConfList[kqpriValues[0]][kqpriValues[2]] = new Array();
					}

					kqpriConfList[kqpriValues[0]][kqpriValues[2]]['stockTypeCode']		= kqpriValues[3];
					kqpriConfList[kqpriValues[0]][kqpriValues[2]]['stockAmount'] 		= kqpriValues[4];
					kqpriConfList[kqpriValues[0]][kqpriValues[2]]['weeksToDelivery'] 	= kqpriValues[5];
				} // end: for

				if (typeof product01 == 'object') {
					product01.doKqpriProductUpdate(kqpriConfList);
				}
			} // end: if

			// Check whether there's a content element specific callback
			// function registered for the current product content element.
			// If yes then call that function.
			if (window.product_xajaxOnCompleteCallbackByHashId) {
				var hashIds = getOwnProperties(product_xajaxOnCompleteCallbackByHashId);

				for (var i = 0; i < hashIds.length; ++i) {
					var onCompleteCallback = product_xajaxOnCompleteCallbackByHashId[hashIds[i]];
					if (typeof onCompleteCallback == 'function') {
						onCompleteCallback();
					} // end: if
				} // end: for
			} // end: if
		} // end: function

		// initiate the xajax call to update the product availabilities
		var updateString = productArtnumList.join('#');

		jQuery(document).ready(function(){
			doKqpriProductupdate(updateString);
		});
		</script>

	

			
    

			

    
    



			
