		<script type="text/javascript">
			(function($){
				if (isTablet()) {
					var previewLinks = $("#carousel_2915693537610").find('a');
					previewLinks.each(function(){
						var link = $(this).attr('href');
						$(this).data('currentLink', link);
						$(this).attr('href','javascript:void(0)');
						var item = $(this).parent();
						var ul = $(this).closest('.articleListContainer');

						Hammer(item[0]).on("doubletap", function(e) {
							e.preventDefault();
							location.replace(window.location.origin + link);
						});

						Hammer(item[0]).on("tap", function(e) {
							e.preventDefault();
							if(ul.data('tapped') == item.data('ordernumber')){
								location.replace(window.location.origin + link);
							}else{
								dmc.InjectThumbnail.inject(item[0]);
								ul.data('tapped', item.data('ordernumber'));
							}
						});
					});
				}
			})($);
		</script>

