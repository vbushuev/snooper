	<script type="text/javascript">
		$(document).ready(function(){
			// avoid pollution of global namespace
			(function($){
				var el = $('#p_2915693537610');
				// preselect Article that has selected baseColor
				var colors = $('#listcontainer').data('colors');
				var selectedColors = colors.split(',');
				if(selectedColors instanceof Array){
					if(selectedColors.length > 0){
						var found = false;
						for(var i=selectedColors.length; i>0 ;i--){
							if(found || selectedColors[i-1] == ""){
								break;
							}
							el.find('.articleColorTileImage').each(function(){
								if( $(this).data('color') == selectedColors[i-1]){
									el.find('.selected').removeClass('selected');
									$(this).addClass('selected');
									var newlink = $(this).parent().attr('href');
									var newImage = $(this).data('bigImage');
									el.find('.urldetail').attr('href', newlink).data('original',newlink);
									el.find('.mainImage').data('original',newImage);
									found = true;
								}
							});
						}
					}
				}
				el.find('img').unveil();
			})($);
		});
	</script>


	
