		$("#" + cssId).html(styleSheet);

	}
}

//
// check if device is android stock browser
//
var navU = navigator.userAgent;
var isAndroidMobile = navU.indexOf('Linux') > -1 && navU.indexOf('Mozilla/5.0') > -1 && navU.indexOf('AppleWebKit') > -1;

var regExAppleWebKit = new RegExp(/AppleWebKit\/([\d.]+)/);
var resultAppleWebKitRegEx = regExAppleWebKit.exec(navU);
var appleWebKitVersion = null;

if(resultAppleWebKitRegEx !== null)
{
	appleWebKitVersion = parseFloat(regExAppleWebKit.exec(navU)[1]);
}

//
// check if it is real chrome or fake user agent of stock browser on (updated) android 4.4
// fake browser has same user agent, but no audiodevice support
//

var isStockFakeChromeBrowser = false;

if(typeof AudioContext == 'undefined' && appleWebKitVersion != null && isAndroidMobile && appleWebKitVersion >= 537)
{
	isStockFakeChromeBrowser = true;
} 


var isAndroidStockBrowser = isAndroidMobile && appleWebKitVersion != null && appleWebKitVersion < 537;

if(isAndroidStockBrowser || isStockFakeChromeBrowser)
{
