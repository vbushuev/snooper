var cssId = 'stockBrowserSpecialCss'; 

//
// create special style sheet in head for stock browsers
//

function createStyleSheetForStockBrowser()
{
	if (!document.getElementById(cssId))
	{
		var head  = document.getElementsByTagName('head')[0];
		var style = document.createElement('style');
		style.id   = cssId;
		style.type = 'text/css';
