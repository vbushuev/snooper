<script type="text/javascript">
	var sociomanticDetailViewCheck = 0;

	function sociomanticDetailViewTrack(uid, productObj, currentArticleVO) {
		sociomantic.sonar.adv['baby-walz-de'].clear();
		window.product = {identifier: currentArticleVO.artNumber};
		sociomantic.sonar.adv['baby-walz-de'].track();
	}

	function sociomanticDetailView() {
		return function(uid, productObj, currentArticleVO) {
			if (sociomanticDetailViewCheck !== 0) {
				clearInterval(sociomanticDetailViewCheck);
			}
			sociomanticDetailViewCheck = setInterval(function() {
				if (typeof sociomantic === 'object' &&
					typeof sociomantic.sonar === 'object' &&
					typeof sociomantic.sonar.adv === 'object' &&
					typeof sociomantic.sonar.adv['baby-walz-de'] === 'object')
				{
					sociomanticDetailViewTrack(uid, productObj, currentArticleVO);
					clearInterval(sociomanticDetailViewCheck);
					sociomanticDetailViewCheck = 0;
				}
			},
			250);
		}
	}

	if (typeof product01 == "undefined" && typeof selfUID != "undefined") {
		var product01 = new dmc.WALZ.Product({"selfUID": selfUID});
	}

	product01 = product01.getInstance();
	product01.onArticleChanged.addHandler(sociomanticDetailView());
</script>

	


