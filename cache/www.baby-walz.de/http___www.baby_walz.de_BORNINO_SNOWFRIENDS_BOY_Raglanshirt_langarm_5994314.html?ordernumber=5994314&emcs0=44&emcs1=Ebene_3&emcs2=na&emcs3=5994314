					$.cookie('searchbacklink', $.data(document.body, 'searchbacklink')[0]+'|'+$.data(document.body, 'searchbacklink')[1]+'|'+$.data(document.body, 'searchbacklink')[2], { path: '/' });
				} else {
					$.cookie('searchbacklink', $.data(document.body, 'searchbacklink')[0]+'|'+$.data(document.body, 'searchbacklink')[1], { path: '/' });
				}
			}
		}
		$('a#pdsPagingPrev').click(setSearchbacklinkCookie);
		$('a#pdsPagingNext').click(setSearchbacklinkCookie);
		</script>

	
