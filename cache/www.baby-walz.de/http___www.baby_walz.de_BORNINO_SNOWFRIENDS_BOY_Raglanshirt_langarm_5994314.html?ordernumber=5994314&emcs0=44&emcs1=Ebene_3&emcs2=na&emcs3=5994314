					function printProduct() {

						if (printPopup && printPopup.closed==false) {
							printPopup.close();
						}

						if (typeof product01 == 'object') {
							// get KQpri data of current selected article
							var KQpriArticleParam = null;
							var KQpriArticleList = kqpridata.split('||');
							for(i = 0; i < KQpriArticleList.length; ++i)
							{
								var KQpriArticleSizePos = KQpriArticleList[i].replace("#",";").indexOf("#")+1;
								if(KQpriArticleList[i].indexOf(product01.getCurrentArticle().artNumber) == 0 && (KQpriArticleList[i].substring(KQpriArticleSizePos,KQpriArticleSizePos+1) == "#" || KQpriArticleList[i].indexOf(product01.getCurrentArticle().artSize) == KQpriArticleSizePos))
								{
									KQpriArticleParam = KQpriArticleList[i].replace(/[#]/g,";");
									break;
								}
							}

							// collect data of selected components
							var componentSelection = "";
							$(".selectDetail").each(function () {
								var rel       = $(this).attr("rel");
								var selection = $(this).val();
								if(typeof rel != 'undefined')
								{
									componentSelection += rel + "||" + selection + ";";
								}
							});
							componentSelection = componentSelection.slice(0,-1); // remove last semicolon
							componentSelection = encodeURI(componentSelection);

							// build url
							var urlPrint = '\x2fMode-fuer-Mama-KindBabymode-0-2-JOberteileLangarmshirts\x2fproduct\x2f2914662648110\x2fgroup\x2f125162627710\x2fL\x2f0\x2fProduktdetail-Print\x2ea909\x2e0\x2ehtml';
							if(urlPrint.indexOf('?') != -1)		urlPrint = urlPrint + "&";
							else								urlPrint = urlPrint + "?";

							urlPrint += "componentSelection=" + componentSelection;
							urlPrint += "#KQpriArticle" + KQpriArticleParam;

							printPopup = openWindow(urlPrint,
									'popup_productPrint',
