		function switchToMobile() {
			var cookieMobile = $.cookie("X-UA-Device-force");

			if (cookieMobile == "desktop") {
				$.cookie('X-UA-Device-force', '', { path: '/', expires: -1 });
				$.cookie('mb3pc', '', { expires: -1 });
				var marker = 'emosmarker=Shop-Version/Desktop zu Mobile';

				if (url.match(/emosmarker\=[^\?]*/)) {
					url = url.replace(/emosmarker\=[^\?]*/, marker);
				} else {
					if (window.location.search != "") {
						url = url + '&';
					} else {
						url = url + '?';
					}
					url = url + marker;
				}
				window.location.replace(url);
			}
		}

		$(document).ready(function() {
