			var ajaxNaviURL = '//baby-walz.de.gauzy.bs2/L/0/Ajax-Group-Navigation.a27468.13.html';

            function mainNaviClose() {
                $('.groupNaviFlyout').css('display', 'none');
                $('.groupNavi ul li').removeClass('hover');
                activeNav = -1;

                checkHover();
            }

            function topNaviClose() {
                $('.tnlButton').removeClass('sfhover');
                $('#'+activeTopNav).css('display','none');
                activeTopNav = -1;
            }

            mouseOverMainNavi = false;
            mouseOverFlyout = false;
            if (isTablet()) {
                $('.groupNaviFlyout').live('mouseenter', function(){
                            mouseOverMainNavi = true;
                        }).live('mouseleave', function(){
                                mouseOverMainNavi = false;
                            });
                $('.groupNaviLevel1').live('mouseenter', function(){
                            mouseOverFlyout = true;
                        }).live('mouseleave', function(){
                                mouseOverFlyout = false;
                            });
                $("html").click(function(){
                        if (!mouseOverMainNavi && !mouseOverFlyout) {
                            mainNaviClose();
                        }
                    });

                $('.tnlButton > a').attr('href','#');
                mouseOverTopNavi = false;
                mouseOverTopNavFlyout = false;
                $('#'+activeTopNav).live('mouseenter', function(){
                            mouseOverTopNavFlyout = true;
                        }).live('mouseleave', function(){
                                mouseOverTopNavFlyout = false;
                            });
                $('.topNaviContainer').live('mouseenter', function(){
                            mouseOverTopNavi = true;
                        }).live('mouseleave', function(){
                                mouseOverTopNavi = false;
                            });
                $("html").click(function(){
                        if (!mouseOverTopNavi && !mouseOverTopNavFlyout) {
                            topNaviClose();
                        }
                    });
            }


		</script>
