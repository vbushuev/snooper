    $jq('#fb_container').on('click', ".fb_activate", function() {
        App.generic.Storage.store('kik_fb_status', 1);
        $jq('.fbActivateBox').hide();

        if($jq('.fb-page').length) {
            $jq('.fbDeActivateBox').show();
        }else{
            $jq('#fb_container .fbDeActivateBox').html(fbCode);
        }

        return false;
    });

    $jq('#fb_container').on('click', ".fb_deactivate", function() {
        App.generic.Storage.store('kik_fb_status', 0);
        $jq('.fbDeActivateBox').hide();
        $jq('.fbActivateBox').show();
        return false;
    });

    $jq(document).ready(function() {
        if (App.generic.Storage.retrieve('kik_fb_status') == 1) {
            $jq('#fb_container .fb_activate').click();
        }
