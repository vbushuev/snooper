	$jq(function() {
		var timeoutId = 0;
		$jq('.service-info .icon.info')
			.on('mouseenter', function(e) {
				var icon = $jq(this);

				clearInterval(timeoutId);
				timeoutId = setTimeout(function() {
					new App.module.TooltipLayer(icon, 'service-info')
						.init(180, 45)
						.fill(Translator.translate('Serviceinfo Tooltip Info'))
						.closeOnMouseLeave();
				}, 250);

				e.stopPropagation();
			})
			.on('mouseleave', function() {
				clearInterval(timeoutId);
			});
	});
