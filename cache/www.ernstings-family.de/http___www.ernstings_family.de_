          jQuery("#oldbrowserLink").click();
          $j.cookie("oldBrowserShow", "true", {expiry: 0, domain: '', path: '/' }); 
        });
      }
      
      if($j.cookie("oldBrowserShow") != "true"){
        var operaBrowserInfo = navigator.userAgent.match(/(OPR)\/(\S+)/ || [,,,]);
        var anotherBrowserInfo = navigator.userAgent.match(/(Safari|Chrome|Firefox|MSIE|Trident)\/(\S+)/ || [,,,]);
        if(operaBrowserInfo){
          if(operaBrowserInfo[2].split('.')[0] <= 12){
            browserOldVersionMessage();
          }
        } else if (anotherBrowserInfo){
          var browserName = anotherBrowserInfo[1];
          var browserVersion = anotherBrowserInfo[2].split(".")[0];
          switch (true) {
            case /Trident/.test(browserName): //MSIE
              if(browserVersion <= 5){
                browserOldVersionMessage();
              }
              break;
            case /Firefox/.test(browserName):
              if(browserVersion <= 15){
              browserOldVersionMessage();
              }
              break;
            case /Chrome/.test(browserName):
              if(browserVersion <= 22){
                browserOldVersionMessage();
              }
              break;
            case /Safari/.test(browserName):
              if(browserVersion < 533){
                browserOldVersionMessage();
              }
              break;
            default:
            break;
          }
        }
      }
